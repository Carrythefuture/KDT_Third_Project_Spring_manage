<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="Board">

    <!--<insert id="insertModel" parameterType="com.tnt.project.dto.ModelDTO">
        
    </insert>-->

    
<select id="findAll" resultType="com.tnt.app.dto.BoardDTO">

    SELECT 
        seq,
        id,
        title,
        text,
        color,
        body_shape,
        image_uuid,
        image_original,
        image_url,
        created_at
    FROM board
    ORDER BY seq DESC
</select>

<delete id="delete" parameterType="int">
	Delete from board where seq=#{seq}

</delete>

</mapper>
